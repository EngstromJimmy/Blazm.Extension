      <Button guid="BlazmExtension" id="QuickSave" priority="0x0300" type="Button">
        <Parent guid="BlazmExtension" id="BlazmExtensionMenuGroup"/>
        <Strings>
          <ButtonText>Quick Save</ButtonText>
        </Strings>
