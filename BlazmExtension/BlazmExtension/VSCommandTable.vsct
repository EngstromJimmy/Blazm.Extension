		<Menus>
			<Menu guid="BlazmExtension" id="BlazmExtensionMenu" priority="0x700" type="Menu">
				<Parent guid="guidSHLMainMenu" id="IDG_VS_MM_TOOLSADDINS" />
				<Strings>
